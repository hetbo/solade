{
    "name": "hetbo/solade",
    "description": "Use Solar Icons Pack With Ease",
    "keywords": ["Blade", "Laravel"],
    "homepage": "https://github.com/hetbo/solade",
    "license": "MIT",
    "authors": [
        {
            "name": "Hosein Safdel"
        }
    ],
    "require": {
        "php": "^8.2",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Hetbo\\Solade\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hetbo\\Solade\\SoladeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
